<p id="notice"><%= notice %></p>

<h1>Listing Horarios</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora inicio</th>
      <th>Hora fin</th>
      <th>Tipo tramite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horarios.each do |horario| %>
      <tr>
        <td><%= horario.fecha %></td>
        <td><%= horario.hora_inicio %></td>
        <td><%= horario.hora_fin %></td>
        <td><%= horario.tipo_tramite %></td>
        <td><%= link_to 'Show', horario %></td>
        <td><%= link_to 'Edit', edit_horario_path(horario) %></td>
        <td><%= link_to 'Destroy', horario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horario', new_horario_path %>
